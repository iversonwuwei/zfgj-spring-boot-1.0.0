		<result column="orderId" property="orderId"/>
		<result column="NAME" property="name" jdbcType="VARCHAR" />
		<result column="PRICE" property="price" jdbcType="DECIMAL" />
		<result column="CYCLE" property="cycle" jdbcType="CHAR" />
		<result column="PAYMENT_NAME" property="paymentName" jdbcType="CHAR" />
		<result column="PAYMENT_TEL" property="paymentTel" jdbcType="VARCHAR" />
		<result column="ADD1" property="add1" jdbcType="VARCHAR" />
		<result column="ADD2" property="add2" jdbcType="VARCHAR" />
		<result column="TIME_TYPE" property="timeType" jdbcType="CHAR" />
		<result column="STATUS" property="status" jdbcType="CHAR" />
		<result column="SALE" property="sale" jdbcType="DECIMAL" />
		<result column="EPRICE" property="eprice" jdbcType="DECIMAL" />
    <association column="ID" property="sysBroadbandLogList" select="com.dlfc.zfgj.mapper.SysBroadbandLogMapper.getTimeList" />
  </resultMap>
  <select id="selectBbAndStatus" parameterType="map" resultMap="MapAndCodeStatus">
		SELECT
		<include refid="Base_Column_List" />
		FROM SYS_BROADBAND
		WHERE DELETE_FLG = 0 AND
		ID = #{id}
	</select>
	<select id="getOrder" parameterType="string" resultMap="MapOrder">
		SELECT
			a.ID           AS ID,
			b.ID           AS orderId,
			b.ORDER_NO     AS orderNo,
			a.STATUS       AS status,
			a.NAME         AS NAME,
			a.CYCLE        AS CYCLE,
			a.PRICE        AS PRICE,
			f.NAME         AS PAYMENT_NAME,
			e.MOBILE       AS PAYMENT_TEL,
			a.ADD1         AS ADD1,
			a.ADD2         AS ADD2,
			a.TIME_TYPE    AS TIME_TYPE,
			a.SALE         AS SALE,
			a.EPRICE       AS EPRICE,
			LEFT(b.ORDER_NO, 2) AS bizNo,
			c.NAME AS installTime
		FROM
			sys_broadband a
		LEFT JOIN sys_order b ON a.ID = b.LID 
		LEFT JOIN sys_code c ON c.TYPE = 'time_type' AND c.`CODE` = a.TIME_TYPE
		LEFT JOIN usr_user e ON e.ID = b.U_ID
		LEFT JOIN sys_person f ON f.ID = e.PER_ID
		WHERE
			a.DELETE_FLG = 0
			and b.DELETE_FLG = 0
			and e.DELETE_FLG = 0
			AND f.DELETE_FLG = 0
			and a.id = #{id}
  </select>
  
  <select id="getById" parameterType="string" resultType="com.dlfc.zfgj.entity.SysBroadband">
	SELECT
		a.id,
		a.eprice,
		a.phone,
		b.order_no,
		left(b.order_no, 2) AS bizNo
	from sys_broadband a
	left join sys_order b on a.id = b.lid
	where a.delete_flg = '0' and a.id = #{id}
  </select>
</mapper>