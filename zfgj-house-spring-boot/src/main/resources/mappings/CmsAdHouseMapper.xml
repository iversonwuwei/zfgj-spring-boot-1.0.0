            B.FILE_PATH,
            D.HOUSE_ADDR
        FROM
            CMS_AD_HOUSE A
            LEFT JOIN SYS_INFO_ATT B ON A.ID = B.LID AND B.FILE_TYPE = 13
            LEFT JOIN HOU_LEASE_INFO C ON A.HOUSE_ID = C.ID
            LEFT JOIN HOU_HOUSE_INFO D ON C.HID = D.ID
