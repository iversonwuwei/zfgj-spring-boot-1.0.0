	where a.delete_flg = '0' and a.area_id = #{areaId}
  </select>

    <select id="getAreaAndTrade" parameterType="com.dlfc.zfgj.entity.SysAreaAreas"
            resultType="com.dlfc.zfgj.entity.SysAreaAreas">
	select 
	a.area_id,
	a.area,
	b.name tradeName
	from sys_area_areas a
	left join sys_trade_areas b on a.area_id = b.parent_id
	where a.delete_flg = '0' and b.trade_id = #{tradeId}
  </select>

