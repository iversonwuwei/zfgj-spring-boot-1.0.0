			LEFT JOIN HOU_LEASE_INFO C ON A.HOUSE_ID = C.ID
			LEFT JOIN HOU_HOUSE_INFO D ON C.HID = D.ID
		<where>
		    A.DELETE_FLG = 0 AND A.ad_id = #{id} and C.RELEASE_STATUS = '0' 
