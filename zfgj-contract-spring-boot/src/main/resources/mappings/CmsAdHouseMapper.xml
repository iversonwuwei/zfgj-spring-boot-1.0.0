	<select id="findHouses" parameterType="com.dlfc.zfgj.entity.CmsAdHouse" resultType="com.dlfc.zfgj.entity.CmsAdHouse" useCache="false">
		SELECT
			A.*,
			B.FILE_PATH,
			D.HOUSE_ADDR
		FROM
			CMS_AD_HOUSE A
			LEFT JOIN SYS_INFO_ATT B  ON  A.ID = B.LID AND B.FILE_TYPE = 13
			LEFT JOIN HOU_LEASE_INFO C ON A.HOUSE_ID = C.ID
			LEFT JOIN HOU_HOUSE_INFO D ON C.HID = D.ID
		<where>
			A.DELETE_FLG = 0 AND A.ad_id = #{adId} and C.RELEASE_STATUS = '1' 
	    	AND B.DELETE_FLG = '0'
	    	AND C.DELETE_FLG = '0'
	    	AND D.DELETE_FLG = '0'
		</where>
			order by a.sort desc
			limit 0,#{sort}  
	</select>
	
	<select id="findUnderHouse" parameterType="java.lang.String" resultType="com.dlfc.zfgj.entity.CmsAdHouse" useCache="false">
